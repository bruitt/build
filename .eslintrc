{
  "parser": "babel-eslint",
  "extends": "@bruitt/eslint-config-lint-base",
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "dot-notation": "warn",
    "max-len": ["error", 80, 2, {
      ignoreUrls: true,
      ignoreComments: false,
      ignoreRegExpLiterals: true,
      ignoreStrings: true,
      ignoreTemplateLiterals: true
    }],
  }
}
