{
  "name": "@bruitt/eslint-config-lint-app",
  "version": "3.0.2",
  "main": "dist/lintApp.js",
  "module": "dist/lintApp.es.js",
  "typings": "dist/types/lintApp.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "mkdir -p ./node_modules/@bruitt && ln -sfn $(pwd) ./node_modules/@bruitt/eslint-config-lint-app",
    "prebuild": "rm -rf dist",
    "build": "tsc && rollup -c",
    "postbuild": "rm -rf compiled",
    "check-ver": "npm-check --skip-unused",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "url": "git@github.com:bruitt/build.git",
    "type": "git"
  },
  "author": "Pavel Pertsev <morhetz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@bruitt/eslint-config-lint-base": "^2.1.3",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.25",
    "npm-check": "^5.4.0",
    "rollup": "^0.49.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "typescript": "^2.4.2"
  }
}
