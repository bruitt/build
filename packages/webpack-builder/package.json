{
  "name": "@bruitt/webpack-builder",
  "version": "4.0.4",
  "main": "dist/index.js",
  "bin": {
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server"
  },
  "scripts": {
    "build": "babel lib -d dist",
    "check-ver": "npm-check --skip-unused",
    "prepublish": "BABEL_ENV=production npm run build",
    "lint": "eslint ./lib",
    "postinstall": "mkdir -p ./node_modules/@bruitt && ln -sfn $(pwd) ./node_modules/@bruitt/webpack-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bruitt/build.git"
  },
  "author": "Pavel Pertsev <morhetz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@bruitt/postcss-bundle": "^1.3.0",
    "babel-cli": "^6.22.0",
    "babel-core": "^6.22.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-ramda": "^1.1.6",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.0.0-rc",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "markdown-loader": "^2.0.0",
    "minimist": "^1.2.0",
    "postcss-loader": "^1.3.0",
    "postcss-scss": "^0.4.0",
    "prerender-spa-plugin": "^2.0.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "stats-webpack-plugin": "^0.4.3",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "0.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.0-rc"
  },
  "devDependencies": {
    "@bruitt/eslint-config-lint-base": "^2.1.1",
    "babel-preset-bruitt-lib": "^1.1.0",
    "npm-check": "^5.4.0"
  }
}
