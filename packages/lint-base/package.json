    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint_d": "^5.1.0",
    "lint-staged": "^4.0.4",
    "prettier": "^1.5.3",
    "prettier_d": "^5.7.2"
